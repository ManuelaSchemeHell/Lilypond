\version "2.19.38"

"cMUSIK" = \relative {
  c' d e f g2 g
}

mu = "MU"

sik = "SIK"

\score {
\new Staff <<
#(ly:parser-include-string 
(string-append "\\\"c" mu sik "\""))
>>
}

mystring="Vom Barette schwankt die Feder"

#(ly:parser-include-string
(string-append "\"m" mystring "\"")) = { c d e }

#(ly:parser-include-string
(string-append "\\\"m" mystring "\""))
