
\bookpart
{
  \markuplist
  {
    \vspace#3.5
    \abs-fontsize #15
    {
      \bold \smallCaps
      "Tabelle aller Dreiklänge der Kreuz-Tonarten"
    }
    \vspace#3.5
    \justify {
      \score
      {
        \new Staff
        \with
        {
          \remove "Time_signature_engraver"
          %\remove "Clef_engraver"
          \remove "Bar_engraver"
        }
        \relative c' {
          <<
            \context Voice  {
              \override Staff.InstrumentName.font-size = #2
              \override Staff.InstrumentName.self-alignment-X = #CENTER
              \set Staff.instrumentName= \markup \left-column { \bold "C-Dur" }
              s1
              \JazzChords < c e geses > < c es geses >
            }
            \new ChordNames
            {
              \germanChords s1
              \JazzChords
            }
          >>
        }
        \layout
        {
          ragged-right=##f
          line-width=#'120
          indent = #'20
        }
      }
    }

    \vspace#1.5
    \justify {
      \score
      {
        \new Staff
        \with
        {
          \remove "Time_signature_engraver"
          %\remove "Clef_engraver"
          \remove "Bar_engraver"
        }
        \relative c' {
          <<
            \context Voice  {
              \override Staff.InstrumentName.font-size = #2
              \override Staff.InstrumentName.self-alignment-X = #CENTER
              \set Staff.instrumentName= \markup \left-column { \bold "G-Dur" }
              s1
              \key g \major
              \transpose c g
              { \JazzChords < c' e' geses' > < c' es' geses' > }
            }
            \new ChordNames
            {
              \germanChords s1
              \key g \major
              \transpose c g
              \JazzChords
            }
          >>
        }
        \layout
        {
          ragged-right=##f
          line-width=#'120
          indent = #'20
        }
      }
    }

    \vspace#1.5
    \justify {
      \score
      {
